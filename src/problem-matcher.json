{
  "problemMatcher": [
    {
      "owner": "sphinx-problem-matcher",
      "pattern": [
        {
          "regexp": "^(\\/.+?):(\\d+): (WARNING|ERROR): (.*)$",
          "file": 1,
          "line": 2,
          "severity": 3,
          "message": 4
        }
      ]
    },
    {
      "owner": "sphinx-problem-matcher-autodoc",
      "pattern": [
        {
          "regexp": "^(\\/.+?):(docstring of .*?: (WARNING|ERROR): .*)$",
          "file": 1,
          "message": 2,
          "severity": 3
        }
      ]
    },
    {
      "owner": "sphinx-problem-matcher-loose",
      "pattern": [
        {
          "regexp": "(\\/.+?\\.rst): (WARNING|ERROR): (.*)$",
          "file": 1,
          "severity": 2,
          "message": 3
        }
      ]
    },
    {
      "owner": "sphinx-problem-matcher-loose-2",
      "pattern": [
        {
          "regexp": "^(WARNING|ERROR): (.*)$",
          "severity": 1,
          "message": 2
        }
      ]
    }
  ]
}
